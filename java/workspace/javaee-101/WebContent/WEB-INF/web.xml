<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <display-name>javaee-101</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    
  </welcome-file-list>
  
  <servlet>
  <servlet-name>BoutDeChou</servlet-name>
  <servlet-class>io.sopra.pox3.servlet.HelloWorldServlet</servlet-class>
  </servlet>
  
  <servlet>
  <servlet-name>Horloge</servlet-name>
  <servlet-class>io.sopra.pox3.servlet.HorlogeServlet</servlet-class>
  </servlet>
  
  <servlet>
  <servlet-name>GetParam</servlet-name>
  <servlet-class>io.sopra.pox3.servlet.GetParamServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>BoutDeChou</servlet-name>
  <url-pattern>/hello.html</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  <servlet-name>Horloge</servlet-name>
  <url-pattern>/horloge.html</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  <servlet-name>GetParam</servlet-name>
  <url-pattern>/getparam.html</url-pattern>
  </servlet-mapping>
</web-app>